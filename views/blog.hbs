<section id="projectsSection">

    {{#each entries}}
        <!-- PROJECTS START -->
        <div class="project-slide-container">
        <div class="project-slide">
            <div class="projInfo">
                <a href="" target="_blank">
                    <p>{{this.title}}</p>
                </a>  
                <p>{{this.content}}</p>
            </div>
            <a href="<!--project link-->>" target="_blank">
                <img src="{{this.image}}">
            </a>    
               
        </div>            <div class="projectTags">
                <p>posted on {{this.date}}</p>
            </div>
            </div>
    {{/each}}
</section>
